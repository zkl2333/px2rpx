{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es6", "esnext", "dom"],
		"module": "es6",
		"moduleResolution": "node",
		"declaration": true,
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": ["./dist"]
}
